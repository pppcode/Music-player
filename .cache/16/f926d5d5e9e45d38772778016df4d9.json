entHub['swipLeft'].forEach(fn=>fn.bind(root)())\n        }  \n      }, 100)\n    }\n\n    this.on = function (type, fn) {\n      if(eventHub[type]) {\n        eventHub[type].push(fn)\n      }\n    }\n\n    this.off = function (type, fn) {\n      let index = eventHub[type].indexOf(fn)\n      if( index !== -1) {\n        eventHub[type].splice(index, 1)\n      }\n    }\n  }\n}\n\n/*\nlet swiper = new Swiper('#box')\nswiper.on('swipLeft', ()=>{\n  console.log('left')\n})\nswiper.on('swipRight', ()=>{\n  console.log('right')\n})\n\nlet onLeft = () => console.log('left 2')\nswiper.on('swipLeft', onLeft)\nswiper.off('swipLeft', onLeft)\n*/\n\n\nexport default Swiper\n\n"},"lineCount":null}},"error":null,"hash":"980870c003ac4055b6bc721da0a51ba4","cacheData":{"env":{}}}